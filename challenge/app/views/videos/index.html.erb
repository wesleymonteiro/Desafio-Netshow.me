<div class="page-header">
  <h1>Videos</h1>
</div>

<div class="col-md-4">
  <%= link_to "Add new", new_video_path, class: "btn btn-success" %>
</div>
<br>

<% unless videos.blank? %>
  <br>
  <div class="table-responsive">
    <table class="table table-striped table-hover">
      <thead>
        <tr>
          <th>Title</th>
          <th>Views</th>
          <th>Creator</th>
          <th></th>
          
      <tbody>
        <% videos.each do |video|  %>
          <%= render "video", video: video %>
        <% end %>
      </tbody>
    </table>
  </div>
<% end %>
